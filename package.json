{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#7d19e430b47efd687b35b6789674b92334d397a7",
    "@angular/compiler": "github:angular/compiler-builds#60996e104d5efd02bfef34d2cea5f7cd7b6924a1",
    "@angular/core": "github:angular/core-builds#e77932ea8853947401a4d12fee4f37f36f5e0688",
    "@angular/forms": "github:angular/forms-builds#ab32de290377792cf1a8dc83a5d081db613c845f",
    "@angular/animations": "github:angular/animations-builds#9fe8b543b33f555fabc1606876cc4ec363da4537",
    "@angular/platform-browser": "github:angular/platform-browser-builds#4bef44058b37f5457ff3b08ee5f65c8e8848ad56",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#ad30c159864814f9fcf8589ba255450a4fadf92d",
    "@angular/cdk": "~9.2.0",
    "@angular/material": "~9.2.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^13.0.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "ngx-flamegraph": "0.0.3",
    "ngx-vis": "3.0.3",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "scroll-into-view-if-needed": "^2.2.22",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/router": "github:angular/router-builds#73578d4471f1a31bf7ea921475aed8962909ae7c",
    "@angular/elements": "github:angular/elements-builds#f60d84ab3cd32e7e56ca4abaefab95db743e4573",
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#4032a546bac2471243e6fa61c2e5a895e3a8f232",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "github:angular/cli-builds#8b09ac76f8989c8b4f3e0bc61c2278c0af25599d",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#f397a99462f689ab0fa6251221e44138ea8d12c5",
    "@angular/language-service": "github:angular/language-service-builds#70cdb38917ac2ddcc83a198484c0406b98fc45db",
    "@types/chrome": "^0.0.101",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.8.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1"
  }
}
