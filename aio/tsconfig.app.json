{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "trusted-types"
    ],
    "plugins": [
      {
        "name": "tsec",
        "exemptionConfig": "./security-exemptions.json"
      }
    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
